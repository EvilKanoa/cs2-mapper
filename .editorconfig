# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Default to unix-style newlines
[*]
end_of_line = lf
insert_final_newline = true

[*.{js,jsx,ts,tsx}]
charset = utf-8
indent_style = tab
indent_size = 2

[{package.json,**/package.json}]
indent_style = tab
indent_size = 2
